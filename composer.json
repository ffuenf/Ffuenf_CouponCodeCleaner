{
  "name": "Ffuenf/Ffuenf_CouponCodeCleaner",
  "license": "MIT",
  "description": "This is a extension for Magento Community Edition that allows you to clean up expired sales rules (coupon codes) in a scheduler task.",
  "authors": [
    {
      "name": "Achim Rosenhagen",
      "email": "a.rosenhagen@ffuenf.de"
    }
  ],
  "type": "magento-module",
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    }
  ],
  "extra": {
    "magento-root-dir": "./magento/",
    "map": [
      [
        "app/code/community/Ffuenf/CouponCodeCleaner",
        "app/code/community/Ffuenf/CouponCodeCleaner"
      ],
      [
        "app/etc/modules/Ffuenf_CouponCodeCleaner.xml",
        "app/etc/modules/Ffuenf_CouponCodeCleaner.xml"
      ],
      [
        "shell/ffuenf_couponcodecleaner.php",
        "shell/ffuenf_couponcodecleaner.php"
      ]
    ]
  }
}